<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="uuzzcircleimageview">
        <attr name="outside_border_thickness" format="dimension"/>
        <attr name="outside_border_distance" format="dimension"/>
        <attr name="outside_border_color" format="color"/>
        <attr name="inside_border_thickness" format="dimension"/>
        <attr name="inside_border_distance" format="dimension"/>
        <attr name="inside_border_color" format="color"/>
        <attr name="content_background_color" format="color"/>
        <attr name="content_color" format="color"/>
        <attr name="content_text" format="string"/>
        <attr name="content_text_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PtrClassicHeader">
        <attr format="integer" name="ptr_rotate_ani_time"/>
    </declare-styleable>

    <declare-styleable name="PtrFrameLayout">
        <attr format="reference" name="ptr_header"/>
        <attr format="reference" name="ptr_content"/>
        <attr format="float" name="ptr_resistance"/>
        <attr format="float" name="ptr_ratio_of_header_height_to_refresh"/>
        <attr format="integer" name="ptr_duration_to_close"/>
        <attr format="integer" name="ptr_duration_to_close_header"/>
        <attr format="boolean" name="ptr_pull_to_fresh"/>
        <attr format="boolean" name="ptr_keep_header_when_refresh"/>
    </declare-styleable>

    <declare-styleable name="RoundAngleImageView">
        <attr name="roundWidth" format="dimension" />
        <attr name="roundHeight" format="dimension" />
    </declare-styleable>
</resources>
